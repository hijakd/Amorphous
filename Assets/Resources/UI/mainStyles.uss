.headBackground {
    background-color: rgba(36, 55, 140, 0.57);
    visibility: hidden;
}

.bodyBackground {
    flex-grow: 1;
    background-color: rgba(154, 38, 38, 0.57);
    display: flex;
    visibility: hidden;
}

.footBackground {
    background-color: rgba(48, 140, 55, 0.57);
    visibility: hidden;
}

.headSize {
    flex-grow: 1;
    height: auto;
    min-height: 5%;
    max-height: 13%;
    flex-direction: row;
}

.headerCell {
    min-width: 20%;
    flex-direction: row;
    flex-grow: 1;
}

.footSize {
    flex-grow: 1;
    max-height: 10%;
    min-height: 5%;
}

.farLeftCellBg {
    background-color: rgba(89, 17, 142, 0.18);
}

.leftCellBg {
    background-color: rgba(140, 55, 3, 0.18);
}

.centerCellBg {
    background-color: rgba(105, 118, 16, 0.18);
}

.rightCellBg {
    background-color: rgba(34, 135, 62, 0.18);
}

.farRightCellBg {
    background-color: rgba(31, 109, 161, 0.18);
}

.centeredCol {
    align-items: center;
    justify-content: center;
    align-self: auto;
    align-content: center;
}

.textBase {
    -unity-font-definition: url("project://database/Assets/TextMesh%20Pro/Examples%20&%20Extras/Fonts/Oswald-Bold.ttf?fileID=12800000&guid=c9f6d0e7bc8541498c9a4799ba184ede&type=3#Oswald-Bold");
    font-size: 18px;
    -unity-text-align: middle-center;
    color: rgb(255, 255, 255);
    -unity-text-outline-width: 1px;
    -unity-font-style: bold;
}

.textTime {
    -unity-font-definition: url("project://database/Assets/TextMesh%20Pro/Examples%20&%20Extras/Fonts/Oswald-Bold.ttf?fileID=12800000&guid=c9f6d0e7bc8541498c9a4799ba184ede&type=3#Oswald-Bold");
    font-size: 26px;
    -unity-text-align: middle-center;
    -unity-text-outline-width: 3px;
    color: rgb(255, 254, 254);
    -unity-font-style: bold;
    text-shadow: 0 0 0 rgba(0, 0, 0, 0);
    padding-right: 7px;
}

.textMenu {
    -unity-font-definition: url("project://database/Assets/TextMesh%20Pro/Examples%20&%20Extras/Fonts/Oswald-Bold.ttf?fileID=12800000&guid=c9f6d0e7bc8541498c9a4799ba184ede&type=3#Oswald-Bold");
    font-size: 28px;
    -unity-text-align: middle-center;
    color: rgb(255, 255, 255);
    -unity-text-outline-width: 1px;
    -unity-font-style: bold;
}

.blip {
    min-width: 75%;
    min-height: 75%;
    border-top-width: 3px;
    border-right-width: 3px;
    border-bottom-width: 3px;
    border-left-width: 3px;
    border-radius: 10px;
    border-left-color: rgb(0, 0, 0);
    border-right-color: rgb(0, 0, 0);
    border-top-color: rgb(0, 0, 0);
    border-bottom-color: rgb(0, 0, 0);
    top: 10%;
    margin-bottom: 5px;
}

.flexGrow {
    flex-grow: 1;
    flex-direction: column;
}

.flexRowGrow {
    flex-direction: row;
    flex-grow: 1;
}

.blipCell {
    max-height: none;
    min-height: 80%;
    min-width: 50%;
    margin: 0;
    padding: 5% 10%;
    justify-content: center;
    align-content: center;
    flex-basis: auto;
}

.farRightBin {
    align-items: center;
    justify-content: space-around;
    align-self: stretch;
    align-content: center;
    flex-direction: row;
    flex-grow: 1;
    min-width: 20%;
}

.farRightBinCell {
    justify-content: center;
    max-height: none;
    min-height: 80%;
    min-width: 50%;
    align-items: center;
    align-content: center;
}

.buttons {
    border-radius: 7px;
    border-top-width: 3px;
    border-right-width: 3px;
    border-bottom-width: 3px;
    border-left-width: 3px;
    border-left-color: rgb(41, 41, 41);
    border-right-color: rgb(41, 41, 41);
    border-top-color: rgb(41, 41, 41);
    border-bottom-color: rgb(41, 41, 41);
    max-width: 85%;
    color: rgb(5, 5, 5);
    -unity-font-definition: url("project://database/Assets/TextMesh%20Pro/Examples%20&%20Extras/Fonts/Oswald-Bold.ttf?fileID=12800000&guid=c9f6d0e7bc8541498c9a4799ba184ede&type=3#Oswald-Bold");
    font-size: 18px;
    -unity-text-align: middle-center;
    padding-right: 13px;
    padding-left: 13px;
}

.settingsPanelAlignment {
    align-items: center;
    justify-content: center;
    align-self: center;
    align-content: center;
    min-width: 33%;
    min-height: 100%;
}

.centeredSettings {
    align-items: center;
    justify-content: space-around;
    min-height: 33%;
    align-self: center;
    min-width: 100%;
    align-content: center;
}

#settingsCenter > #enableHints #unity-checkmark {
    min-width: 66%;
    min-height: 66%;
    border-radius: 7px;
}

#settingsCenter > #enableHints > .unity-toggle__input {
    align-items: stretch;
    align-self: center;
    justify-content: center;
    flex-grow: 0;
    min-width: 16%;
    min-height: 27%;
}

#settingsCenter > #difficulty > .unity-base-field__input {
    justify-content: center;
    align-self: center;
    flex-grow: 0;
}

#settingsMenu > #settingsCenter > #difficulty > .unity-base-field__input > #unity-radio-button-group__container > .unity-radio-button > .unity-radio-button__input > .unity-radio-button__checkmark-background {
    margin-right: 21px;
}

#settingsMenu > #settingsCenter > #playerSpeed > .unity-slider__input {
}

#playerSpeed > .unity-slider__label {
    top: 33%;
    margin-bottom: 13%;
}

.fullScreen {
    width: 100%;
    height: 100%;
    position: absolute;
}

#playerSpeed > .unity-slider__input {
    padding-right: 7%;
    padding-left: 7%;
}

#playerSpeed > .unity-slider__input > #unity-drag-container {
    flex-grow: 1;
    border-left-color: rgba(168, 34, 34, 0.82);
    border-right-color: rgba(168, 34, 34, 0.82);
    border-top-color: rgba(168, 34, 34, 0.82);
    border-bottom-color: rgba(168, 34, 34, 0.82);
    align-self: center;
}

#playerSpeed > .unity-slider__input > #unity-drag-container > #unity-tracker {
    min-height: 13px;
    margin-top: 0;
    border-radius: 7px;
    background-color: rgb(154, 166, 85);
}

#playerSpeed > .unity-slider__input > #unity-drag-container > #unity-dragger {
    margin-top: -6px;
    min-height: 13%;
    min-width: 7%;
    flex-grow: 1;
    border-radius: 7px;
}

#playerSpeed > .unity-slider__input > #unity-drag-container > #unity-tracker > #unity-fill {
    border-radius: 7px;
    background-color: rgb(140, 18, 166);
}

.settingsScreen {
    align-items: stretch;
    justify-content: center;
    align-self: center;
    align-content: center;
    flex-direction: row;
    flex-grow: 1;
    background-color: rgba(0, 0, 0, 0.98);
}

.winQuitScreens {
    align-items: center;
    justify-content: center;
    background-color: rgba(30, 20, 46, 0.98);
}

.winText {
    -unity-font-definition: url("project://database/Assets/TextMesh%20Pro/Examples%20&%20Extras/Fonts/Bangers.ttf?fileID=12800000&guid=5dd49b3eacc540408c98eee0de38e0f1&type=3#Bangers");
    font-size: 72px;
    -unity-text-outline-color: rgb(0, 0, 0);
    color: rgb(211, 255, 0);
    -unity-text-outline-width: 3px;
    text-shadow: 7px 7px 4.2px rgba(80, 80, 80, 0.9);
    letter-spacing: 4px;
    top: -13%;
}

.quitText {
    -unity-font-definition: url("project://database/Assets/TextMesh%20Pro/Examples%20&%20Extras/Fonts/Bangers.ttf?fileID=12800000&guid=5dd49b3eacc540408c98eee0de38e0f1&type=3#Bangers");
    font-size: 72px;
    -unity-text-outline-color: rgb(0, 0, 0);
    color: rgb(211, 255, 0);
    -unity-text-outline-width: 3px;
    text-shadow: 7px 7px 4.2px rgba(80, 79, 79, 0.9);
    letter-spacing: 4px;
}
