            <Bindings>
                <engine:DataBinding property="value" data-source-path="ID" data-source-type="Waypoint, Assembly-CSharp" binding-mode="ToSource" />
            </Bindings>
        </engine:TextField>
    </engine:VisualElement>
</engine:UXML>
